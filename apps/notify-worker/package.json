{
  "name": "notify-worker",
  "version": "1.0.0",
  "description": "Notification processing worker for Streamix",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run db:generate && tsx watch src/index.ts",
    "build": "npm run db:generate && tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "setup": "./scripts/setup.sh",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --schema=../web/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=../web/prisma/schema.prisma"
  },
  "keywords": ["notifications", "worker", "redis", "postgres"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "ioredis": "^5.3.2",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "prisma": "^5.22.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
